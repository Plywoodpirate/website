{
  "name": "michagrandel/pagetemplate",
  "description": "Template for a professional page setup based on docker and composer",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    },
    {
      "type": "path",
      "url": "./packages/*"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "gridelementsteam/gridelements": "^9.2",
    "helhum/config-loader": "^0.1",
    "helhum/dotenv-connector": "^1.0",
    "typo3/cms-belog": "^9.5",
    "typo3/cms-beuser": "^9.5",
    "typo3/cms-core": "^9.5",
    "typo3/cms-extensionmanager": "^9.5",
    "typo3/cms-felogin": "^9.5",
    "typo3/cms-filelist": "^9.5",
    "typo3/cms-fluid-styled-content": "^9.5",
    "typo3/cms-form": "^9.5",
    "typo3/cms-indexed-search": "^9.5",
    "typo3/cms-info": "^9.5",
    "typo3/cms-install": "^9.5",
    "typo3/cms-lowlevel": "^9.5",
    "typo3/cms-recordlist": "^9.5",
    "typo3/cms-redirects": "^9.5",
    "typo3/cms-rte-ckeditor": "^9.5",
    "typo3/cms-scheduler": "^9.5",
    "typo3/cms-seo": "^9.5",
    "typo3/cms-setup": "^9.5",
    "typo3/cms-tstemplate": "^9.5",
    "typo3/cms-viewpage": "^9.5",
    "typo3/cms-workspaces": "^9.5",
    "sjbr/static-info-tables": "^6.9"
  },
  "require-dev": {
    "friendsoftypo3/extension-builder": "^9.10",
    "typo3/testing-framework": "^4.15"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "public"
    }
  }
}
